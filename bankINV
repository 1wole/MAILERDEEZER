local user = _G.RobloxID

local BankClient = game.Players.LocalPlayer.PlayerScripts.Scripts.GUIs['Bank Client']
if not BankClient then
    print("Unable to find Bank Client object")
    return
end

local Libary = require(game.ReplicatedStorage:WaitForChild('Framework'):WaitForChild('Library'))

local MyBanks = getconstants(BankClient.GetMyBanks)[3]
local Invite = getconstants(BankClient.InviteToBank)[18]

local Invoke = (getupvalue or debug.getupvalue)(Libary.Network.Invoke, 2)
local FireFunction = function(n, ...) return Invoke(n):InvokeServer(...) end

local Bank = FireFunction(MyBanks)

FireFunction(Invite, Bank[1]['BUID'], user)
